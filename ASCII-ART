def text_to_ascii(text):
    ascii_codes = []

    for char in text:
        ascii_code = ord(char)  # Get the ASCII code for the character
        ascii_codes.append(ascii_code)

    return ascii_codes

if __name__ == "__main__":
    input_text = input("Enter the text to convert to ASCII: ")
    ascii_codes = text_to_ascii(input_text)

    print("ASCII codes for the text:")
    for code in ascii_codes:
        print(code, end=" ")
